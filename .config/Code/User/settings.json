{
  // Editor settings
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.detectIndentation": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.matchBrackets": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestionsDelay": 25,
  "editor.renderWhitespace": "none",
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 4,
  "editor.wordWrap": "on",
  "editor.find.autoFindInSelection": "always",
  "editor.minimap.enabled": false,
  // Terminal settings
  "terminal.integrated.fontSize": 18,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.py": "python",
    ".bash_functions": "shellscript",
    ".docker_functions": "shellscript",
    ".vimrc": "shellscript"
  },
  "files.exclude": {
    "**/.coverage": true,
    "**/.eggs": true,
    "**/.env": true,
    "**/.envs": true,
    "**/.gitignore": true,
    "**/.gitmodules": true,
    "**/.hound.yml": true,
    "**/.idea": true,
    "**/.pyc": true,
    "**/.pylintrc": true,
    "**/.venv": true,
    "**/.vscode": true,
    "**/*.pyc": true,
    "**/*.swp": true
  },
  "git.confirmSync": false,
  "workbench.statusBar.visible": true,
  "window.titleBarStyle": "custom",
  "diffEditor.renderSideBySide": false,
  "explorer.confirmDelete": false,
  // Search features
  "search.showLineNumbers": true,
  "search.smartCase": true,
  // Language Settings
  "python.pythonPath": "python",
  "editor.gotoLocation.multipleImplementations": "gotoAndPeek",
  "python.linting.maxNumberOfProblems": 50,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.autoUpdateLanguageServer": true,
  "python.linting.ignorePatterns": [
    ".vscode/*.py",
    "**/site-packages/**/*.py",
    ".git/**",
    "**/__pycache__/**",
    "**/node_modules/**"
  ],
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    },
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.rulers": [
      90,
      120
    ],
    "editor.tabSize": 4
  },
  // Spelling and Grammar
  "cSpell.language": "en-uk",
  "cSpell.userWords": [
