    "Log": {
      "parser": "markdown"
    }
  },
  "editor.suggestSelection": "first",
  "editor.renderControlCharacters": false,
  "breadcrumbs.enabled": true,
  "window.menuBarVisibility": "classic",
  "todo-tree.highlights.enabled": false,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "dockerfile",
    "git-commit",
    "go",
