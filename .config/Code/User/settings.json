{
    "breadcrumbs.enabled": true,
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.codeLens": true,
    "editor.comments.insertSpace": true,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.detectIndentation": true,
    "editor.folding": true,
    "editor.fontSize": 12,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.matchBrackets": "always",
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestionsDelay": 25,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.showFoldingControls": "mouseover",
    "editor.showUnused":true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.wordBasedSuggestions": false,
    "editor.wordWrap": "on",
    "files.associations": {
        "*.py": "python",
        ".bash_functions": "shellscript",
        ".docker_functions": "shellscript",
        ".vimrc": "shellscript"
    },
    "files.exclude": {
        "**/.coverage": true,
        "**/.eggs": true,
        "**/.env": true,
        "**/.envs": true,
        "**/.gitignore": true,
        "**/.gitmodules": true,
        "**/.hound.yml": true,
        "**/.idea": true,
        "**/.pyc": true,
        "**/.pylintrc": true,
        "**/.venv": true,
        "**/.vscode": true,
        "**/*.pyc": true,
        "**/*.swp": true
    },
    "diffEditor.renderSideBySide": false,
    "editor.gotoLocation.multipleImplementations": "goto",
    "explorer.confirmDelete": false,
    "extensions.autoUpdate": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autorefresh": false,
    "git.confirmSync": false,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true
    },
    "gitlens.views.repositories.autoRefresh": false,
    "guides.enabled": false,
    "jenkins.pipeline.linter.connector.user": "mmphego",
    "jenkins.pipeline.linter.connector.url": "http://ci.camlab.kat.ac.za/pipeline-model-converter/validate",
    "jenkins.pipeline.linter.connector.crumbUrl": "http://ci.camlab.kat.ac.za/crumbIssuer/api/xml?xpath=concat(//crumbRequestField,%22:%22,//crumb)",
    "python.autoUpdateLanguageServer": true,
    "python.linting.flake8Enabled": true,
    "python.linting.maxNumberOfProblems": 50,
    "python.linting.pylintEnabled": false,
    "python.pythonPath": "python",
    "redhat.telemetry.enabled": false,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "terminal.integrated.fontSize": 14,
    "python.linting.ignorePatterns": [
        ".vscode/*.py",
        "**/site-packages/**/*.py",
        ".git/**",
        "**/__pycache__/**",
        "**/node_modules/**"
    ],
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": false
        },
        "editor.formatOnPaste": true,
        "editor.formatOnSave": false,
        "editor.rulers": [
            90,
            120
        ],
        "editor.tabSize": 4
    },
    "python.defaultInterpreterPath": "python",
    "python.formatting.provider": "black",
    "python.venvFolders": [
        "/home/mmphego/.venvs"
    ],
    "python.venvPath": "/home/mmphego/.venvs",
    "python.linting.flake8Args": [
        "--max-line-length=90"
    ],
    "python.formatting.blackArgs": [
        "-l",
        "90"
    ],
    "python.formatting.blackPath": "/home/mmphego/.local/bin/black",
    "python.languageServer": "Pylance",

    "typescript.updateImportsOnFileMove.enabled": "always",
    "todo-tree.highlights.enabled": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path.linux": "/home/mmphego/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "workbench.statusBar.visible": true,
    "window.menuBarVisibility": "classic",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 2,
    "workbench.colorTheme": "NarutoDark",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    },
    "remove-tabs-on-save.ignoreFileExtensions": [
        "*.go",
        "*Makefile*"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "window.zoomLevel": 1,
    "liveshare.presence": true,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.schemas": {},
    "aws.profile": "profile:default",
    "go.toolsManagement.autoUpdate": true,
    "git.enableSmartCommit": true,
}
