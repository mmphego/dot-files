{
    "editor.autoClosingBrackets": "always",
    "editor.codeLens": true,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.detectIndentation": true,
    "editor.find.autoFindInSelection": "always",
    "editor.folding": true,
    "editor.fontSize": 12,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.insertSpaces": true,
    "editor.inlineSuggest.enabled": true,
    "editor.matchBrackets": "always",
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestionsDelay": 25,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.rulers": [90, 120],
    "editor.showFoldingControls": "mouseover",
    "editor.snippetSuggestions": "top",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "guides.enabled": false,
    "redhat.telemetry.enabled": false,
    "tabnine.experimentalAutoImports": true,
    "terminal.integrated.fontSize": 18,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "NarutoDark",
    "files.associations": {
        "*.py": "python",
        ".bash_functions": "shellscript",
        ".docker_functions": "shellscript",
        ".vimrc": "shellscript"
    },
    "files.exclude": {
        "**/.coverage": true,
        "**/.eggs": true,
        "**/.env": true,
        "**/.envs": true,
        "**/.gitignore": true,
        "**/.gitmodules": true,
        "**/.hound.yml": true,
        "**/.idea": true,
        "**/.pyc": true,
        "**/.pylintrc": true,
        "**/.venv": true,
        "**/.vscode": true,
        "**/*.pyc": true,
        "**/*.swp": true
    },
    "git.confirmSync": false,
    "workbench.statusBar.visible": true,
    "window.titleBarStyle": "custom",
    "diffEditor.renderSideBySide": false,
    "explorer.confirmDelete": false,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "python.pythonPath": "python",
    "editor.gotoLocation.multipleImplementations": "goto",
    "python.linting.maxNumberOfProblems": 50,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.autoUpdateLanguageServer": true,
    "python.linting.ignorePatterns": [
        ".vscode/*.py",
        "**/site-packages/**/*.py",
        ".git/**",
        "**/__pycache__/**",
        "**/node_modules/**"
    ],
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": false
        },
        "editor.formatOnPaste": true,
        "editor.formatOnSave": false,
        "editor.rulers": [
            90,
            120
        ],
        "editor.tabSize": 4
    },
    "cSpell.language": "en",
