    "python.venvFolders": [
      "/home/mmphego/.venvs"
    ],
    "python.venvPath": "/home/mmphego/.venvs",
    "python.linting.flake8Args": [
        "--max-line-length=90"
    ],

    "git.autorefresh": false,
    "gitlens.views.repositories.autoRefresh": false,
    "extensions.autoUpdate": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "python.formatting.blackPath": "/home/mmphego/.local/bin/black",
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "python.defaultInterpreterPath": "python",
    "cSpell.enableFiletypes": [
        "jenkinsfile"
    ],
    "jenkins.pipeline.linter.connector.user": "mmphego",
    "jenkins.pipeline.linter.connector.url": "http://ci.camlab.kat.ac.za/pipeline-model-converter/validate",
    "jenkins.pipeline.linter.connector.crumbUrl": "http://ci.camlab.kat.ac.za/crumbIssuer/api/xml?xpath=concat(//crumbRequestField,%22:%22,//crumb)",
    "remove-tabs-on-save.ignoreFileExtensions": [
        "*.go"
    ]
}
